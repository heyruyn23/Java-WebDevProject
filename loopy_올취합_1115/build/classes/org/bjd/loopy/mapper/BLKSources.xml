<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BLKSources">
	
	<select id="selectSourceCnt" resultType="int" parameterType="int">
		SELECT COUNT(*) soundSourceCnt
		FROM blk_source
		WHERE song_no=#{no}
	</select>
	
</mapper>